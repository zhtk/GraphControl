<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

    <system.serviceModel>
        <services>
            <service name="Driver1.Server">
                <endpoint
					          address="net.tcp://localhost:40000/Driver1"
					          binding="netTcpBinding"
					          contract="Interface.IDriver"
					          bindingConfiguration="InsecureTcp" />
            </service>
        </services>

        <bindings>
            <netTcpBinding>
                <binding name="InsecureTcp">
                    <security mode="None" />
                </binding>
            </netTcpBinding>
        </bindings>
    </system.serviceModel>
</configuration>