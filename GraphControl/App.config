<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <system.serviceModel>
    <client>
      <endpoint
				name="Server1"
				address="net.tcp://localhost:40000/Driver1S1"
				binding="netTcpBinding"
				contract="Interface.IDriver"
				bindingConfiguration="InsecureTcp" />
      <endpoint
				name="Server2"
				address="net.tcp://localhost:40000/Driver1S2"
				binding="netTcpBinding"
				contract="Interface.IDriver"
				bindingConfiguration="InsecureTcp" />
      <endpoint
				name="Server3"
				address="net.tcp://localhost:40001/Driver2"
				binding="netTcpBinding"
				contract="Interface.IDriver"
				bindingConfiguration="InsecureTcp" />
      <endpoint
				name="Switch1"
				address="net.tcp://localhost:40000/Driver1S3"
				binding="netTcpBinding"
				contract="Interface.IDriver"
				bindingConfiguration="InsecureTcp" />
    </client>

    <bindings>
      <netTcpBinding>
        <binding name="InsecureTcp">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>